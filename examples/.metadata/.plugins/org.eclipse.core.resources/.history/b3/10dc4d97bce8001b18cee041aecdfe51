package introduction;

public class Swap {

	public static void main(String[] args) 
	{
		int no1=20;
		int no2=10;
		
		//before swapping
		
		System.out.println("first no:"+no1);
		System.out.println("second no:"+no2);

		int temp=no1;
		
		 no1 =no2;
		
		//after swapping
		
		System.out.println("first no:"+no1);
		System.out.println("second no:"+no2);
		
		
		
		
	}

}
